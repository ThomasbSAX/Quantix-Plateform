{
  "datasets": {
    "left": {
      "path": "data/generated_dirty_cleaner_test.csv"
    }
  },
  "operations": [
    {
      "op": "clean",
      "args": {
        "auto_detect_types": true,
        "convert_units": true,
        "unit_mode": "add",
        "unit_parse_threshold": 0.25,
        "mask_sensitive_data": true
      }
    },
    {
      "op": "add_contact_and_id_features",
      "args": {
        "prefix": "has_"
      }
    },
    {
      "op": "add_unit_columns",
      "args": {
        "source": "distance_vitesse",
        "target_unit": "m",
        "exclude_currency": true
      }
    },
    {
      "op": "apply_features",
      "features": [
        {
          "name": "comment_has_html",
          "kind": "regex_flag",
          "source": "Commentaire",
          "pattern": "<[^>]+>"
        }
      ]
    },
    {
      "op": "save",
      "path": "exports/pipeline_example/out.csv"
    }
  ],
  "output": {
    "report_path": "exports/pipeline_example/report.json"
  }
}
