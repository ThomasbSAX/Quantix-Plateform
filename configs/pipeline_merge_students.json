{
  "datasets": {
    "left": {
      "path": "data/test.csv"
    },
    "right": {
      "path": "data/test_num.csv"
    }
  },
  "operations": [
    {
      "op": "replace_in_column",
      "args": {
        "column": "student_id",
        "pattern": "^\\$",
        "repl": "S",
        "regex": true
      }
    },
    {
      "op": "merge",
      "args": {
        "how": "inner",
        "on": "student_id",
        "indicator": true
      }
    },
    {
      "op": "clean",
      "args": {
        "auto_detect_types": true,
        "normalize_spaces": true,
        "trim_strings": true
      }
    },
    {
      "op": "save",
      "path": "exports/pipeline_merge_students/out.csv"
    }
  ],
  "output": {
    "report_path": "exports/pipeline_merge_students/report.json"
  }
}
